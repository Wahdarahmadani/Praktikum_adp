def generate_output():
    count_dor = 0
    for i in range(1, 101):
        if i % 10 == 3 or i % 10 == 6 or i % 10 == 9:
            print("DOR", end=" ")
            count_dor += 1
        else:
            print(i, end=" ")
        if i % 10 == 0:
            print()
    return count_dor

total_dor = generate_output()
print("\nTotal DOR yang muncul sebanyak =", total_dor)
